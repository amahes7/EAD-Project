<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<managed-bean-name>dbAccess</managed-bean-name>
		<managed-bean-class>f18g312.DbAccess</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>regression</managed-bean-name>
		<managed-bean-class>f18g312.Regression</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>descStatistics</managed-bean-name>
		<managed-bean-class>f18g312.DescStatistics</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>menu</managed-bean-name>
		<managed-bean-class>f18g312.Menu</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>f18g312.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Export</managed-bean-name>
		<managed-bean-class>f18g312.Export</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>f18g312.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>DbOperations</managed-bean-name>
		<managed-bean-class>f18g312.DbOperations</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>f18g312.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>statisticsBean</managed-bean-name>
		<managed-bean-class>f18g312.StatisticsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>f18g312.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>regression</property-name>
			<property-class>f18g312.Regression</property-class>
			<value>#{regression}</value>
		</managed-property>
		<managed-property>
			<property-name>descStatistics</property-name>
			<property-class>f18g312.DescStatistics</property-class>
			<value>#{descStatistics}</value>
		</managed-property>
		<managed-property>
			<property-name>dbOperations</property-name>
			<property-class>f18g312.DbOperations</property-class>
			<value>#{dbOperations}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>visualAnalysis</managed-bean-name>
		<managed-bean-class>f18g312.VisualAnalysis</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>f18g312.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>descStatistics</property-name>
			<property-class>f18g312.DescStatistics</property-class>
			<value>#{descStatistics}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>logout</managed-bean-name>
		<managed-bean-class>f18g312.Logout</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>f18g312.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>statisticsBean</property-name>
			<property-class>f18g312.StatisticsBean</property-class>
			<value>#{statisticsBean}</value>
		</managed-property>
		<managed-property>
			<property-name>dbOperations</property-name>
			<property-class>f18g312.DbOperations</property-class>
			<value>#{dbOperations}</value>
		</managed-property>
		<managed-property>
			<property-name>graphics</property-name>
			<property-class>f18g312.Graphics</property-class>
			<value>#{graphics}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>graphics</managed-bean-name>
		<managed-bean-class>f18g312.Graphics</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>f18g312.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>visualAnalysis</property-name>
			<property-class>f18g312.VisualAnalysis</property-class>
			<value>#{visualAnalysis}</value>
		</managed-property>
		<managed-property>
			<property-name>statisticsBean</property-name>
			<property-class>f18g312.StatisticsBean</property-class>
			<value>#{statisticsBean}</value>
		</managed-property>
	</managed-bean>

	<navigation-rule>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/mainMenu.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/databaseOptions.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/descriptiveStats.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/graphicalAnalysis.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/import.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/mainMenu.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>